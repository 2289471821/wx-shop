.tabs .tabs_title {
  display: flex;
}
.tabs .tabs_title .title_item {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  border-bottom: 5rpx solid #fff;
  padding: 15rpx 0;
}
.tabs .tabs_title .active {
  color: var(--themeColor);
  border-bottom: 5rpx solid currentColor;
}
